<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="alert alert-success alert-dismissible fade show" role="alert">
                Your hash is correct and no one manipulated your data.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <div class="caption mb-2">Your answers</div>
            <div class="list-group">
                <% for (var i = 0; i < answers.length ; i++) { %>
                  <h4 class="mt-2"> <%= i+1 %>. <%= polls[i] %></h4>
                      <h5 class="list-group-item list-group-item">Answer: <%= answers[i] %></h5>
                  <% } %>
            </div>
        </div>
    </div>
  </div>
  <a href="/dashboard" class="btn btn-secondary mt-2">Back</a>
  
